{
  "appId": "com.cam.gps.camera",
  "appName": "Cam",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "Geolocation": {
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "Camera": {
      "permissions": [
        "android.permission.CAMERA"
      ]
    }
  }
}
